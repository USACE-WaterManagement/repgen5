STRSTOR = Value(STRSTRS.last())
##                                 Define all STORAGE CHANGES
PAOSDEL = Value(PAOSTOR-PAOSTRS[BASDATE-timedelta(days=1)])
BLMSDEL = Value(BLMSTOR-BLMSTRS[BASDATE-timedelta(days=1)])
VALSDEL = Value(VALSTOR-VALSTRS[BASDATE-timedelta(days=1)])
