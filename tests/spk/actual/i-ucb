##                                 Define DAILY INCREMENTAL PRECIP
##  %BLMPPT
##  TIME=%BASDATE      PICTURE=NNNNZ.ZZ            MISSTR="    -NR-"
##  FILE=/dat0/rtsc7db.dss             TYPE=DSS
##  A=GUNNISON       B=BLUE MESA    C=PRECIP-INC     E=1DAY      F=
##  %VALPPT
##  A=LOS PINOS      B=VALLECITO
##  %PAOPPT
##  A=MUDDY CR    B=PAONIA
#ENDDEF
