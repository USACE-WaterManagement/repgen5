#DEF
# !ECHO
Value.shared["missing"] = "EXACT"
CTM.picture = "%d %b %Y @ %K%M"
Value.shared["picture"] = "%d %b %Y @ %K%M"
BASDATE.value = BASDATE.value.replace(hour=0,minute=0,second=0,microsecond=0) + timedelta(days=1)    # Add a day to offset setting the time to 00, instead of 24
BASDATE.picture = "%d %b %Y"
Value.shared["picture"] = "%d %b %Y"
FDATE = Value(BASDATE,
	PICTURE="%m/%d/%y",
)
#                                 Define all ELEVATIONS
PAOELEV = Value(
	dbtype="cda",
	DBTZ="MST7MDT",
	DBLOC="Paonia",
	DBPAR="Elev",
	DBPTYP="Inst",
	DBINT="~1Day",
	DBDUR=0,
	DBVER="Raw-USBRSLC",
	TIME=BASDATE,
	PICTURE="%7.2f",
	MISSTR="   -NR-",
	UNDEF="   -NR-",
	DBUNITS="ft",
)
