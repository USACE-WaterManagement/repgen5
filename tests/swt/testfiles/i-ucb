	TIME=%BASDATE                     PICTURE=NNNNNNZ             MISSTR="   -NR-"
#FILE=/dat0/rtsc7db.dss         TYPE=DSS
#A=MUDDY CR       B=PAONIA         C=FLOW-RES IN  E=1DAY         F=
